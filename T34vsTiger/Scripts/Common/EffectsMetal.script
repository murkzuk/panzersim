//Calibre 7576_85_88
class CCalibre7576_85_88BulletMetalHitSplashEffect
  extends CAnimatedParticleGenerator
{
  CCalibre7576_85_88BulletMetalHitSplashEffect()
  {
    SetCoordSystemType(CRDSYS_Local);

    SetAnimationTime(0.5);
    SetEffectTime(0.5);
    SetPlaceParams(0.2);

    SetBaseColor(new Color(1.0, 0.5, 0.0, 0.5));
    SetSizeParams(0.4, 0.2);
    SetStartAndFinalFade(0.0, 0.4, 0.5);
    for (int I = 0; I < 4; I++)
    {
      SetAnimLength(48);
      SetFixedMaterial(new String(I));

      GenerateAnimatedParticle(
          CEffectsArray::SplashCloudLiteEffectSkin, 0, 1.0,
          new Vector(0.0, 0.0, 0.1), new Vector(0.0, 0.0, 1.0 + I),
          new Vector(0.0, 0.0, 0.0), new Vector(5.0 + I, 5.0 + I, 4.0 + I)
        );
    }

  }
}

class CCalibre7576_85_88BulletMetalHitSmokeEffect
 extends CAnimatedParticleGenerator
{
