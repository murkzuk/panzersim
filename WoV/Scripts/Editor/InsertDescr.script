class CInsertObjectDescr
{
  CInsertObjectDescr(
      String _ComponentID,
      String _ScriptClass,
      String _IconFilename
    )
  {
    m_ComponentID = _ComponentID;
    m_ScriptClass = _ScriptClass;
    m_IconFilename = _IconFilename;
  }

  String m_ComponentID;
  String m_ScriptClass;
  String m_IconFilename;

  String GetIconFilename()
  {
    return "Icons\\" + m_IconFilename;
  }
}
