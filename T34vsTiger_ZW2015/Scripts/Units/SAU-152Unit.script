  final static String DefaultHealthParameter = "HP_HEALTHY";  // fragile constitution
  final static int    UnitMenacePower        = MENACE_KILLER;  // light weapon
  final static Array  AutomaticClassificators  = ["GROUND", "TANK", "SAU", "RU"];
