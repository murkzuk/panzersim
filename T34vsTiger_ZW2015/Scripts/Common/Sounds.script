class CEngineSound
{
  float   MinDistance    = 1.0;
  float   MaxDistance    = 80.0;
  String  SoundFile      = "";
  int     MinMaxType     = SD_MAX;

  boolean PlayOnMove     = false;
  Vector  SpeedThreshold = new Vector(0.5, 0.5, 100.0);

  boolean Preload          = true;      // Flag of creating a buffer for sound recording in CSoundArray  Флаг создания буфера для звука при регистрации в CSoundArray
  int Priority             = DEFAULT_SOUND_PRIORITY;     // Приоритет звука (min..max)= (0..0xFFFFFFFF)
  int MaxPlayingSameSounds = 120;         // Max number of simultaneously playing the same sounds Max Количество одновременно играющих одинаковых звуков

  int     TimeLoop         = 0;//ms   If the value is nonzero - playing in a loop within the specified time   // Если значение ненулевое - проигрывание в цикле в течение указанного времени
}

