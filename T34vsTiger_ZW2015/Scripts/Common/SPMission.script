//
// Base class for all single missions
//

class CSPMission
  extends CMission
{
  //
  // Contruction and initialization
  //

  CSPMission(
      String _MissionObjectClass,
      String _MissionContentClass
    )
  {
    CMission(_MissionObjectClass, _MissionContentClass);
  }
}


class CSPBMission
  extends CMission
{
  //
  // Contruction and initialization
  //

  CSPMission(
      String _MissionObjectClass,
      String _MissionContentClass,
      String _MissionContentClass
    )
  {
    CMission(_MissionObjectClass, _MissionContentClass, _MissionContentClass);
  }
}