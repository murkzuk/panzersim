class CBasePhrases
{
  final static Array Sequences = [
   ];

  static Array GetPhrase(
      String _PhraseID
    )
  {
    for (int i = 0; i < Sequences.size(); i ++)
    {
      if (Sequences[i][0] == _PhraseID)
        return Sequences[i];
    }
    logError("can't find PhraseID" + _PhraseID);
    return [];
  }
 
}

class CM1Phrases
  extends CBasePhrases
{
  final static Array Sequences = [
      //Dialogue 1
      ["Temp1", "str_Temp1", "temp1"],
      ["Temp2", "str_Temp2", "temp2"]
   ];
}
