class CCalibre7576_85_88BulletGroundHitEffect
  extends CBaseEffect
{
  CCalibre7576_85_88BulletGroundHitEffect()
  {
    CreateEffect("LightExplosionSplash",         0.0);
    CreateEffect("APGroundSplashRefuseEffect",   0.0);
    CreateEffect("APGroundDustRefuseEffect",     0.0);
    CreateEffect("APGroundDustSinkRefuseEffect", 0.1);
    CreateEffect("APGroundSmokeRefuseEffect",    0.2);
    CreateEffect("APGroundSprayRefuseEffect",    0.1);
    CreateAndForgetEffect("Calibre7576_85_88GroundSignEffect", 0.0);
  }
}
